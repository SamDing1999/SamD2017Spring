package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
	private int col;
	private int row;
	private String location;
    @Override
    public int getRow()
    {
        
        return this.row;
    }

    @Override
    public int getCol()
    {
        return this.col;
    }
    
    public SpreadsheetLocation(String cellName)
    {
    	location = cellName;
    	
    	
    }

}
