package textExcel;

public class TextCell implements Cell {

	private String content;
	
	public TextCell(String s) {
		content = s;
	}

	
	public String abbreviatedCellText() {
		String abbreviated = content.substring(0, 10);
		return abbreviated;
	}

	
	public String fullCellText() {
		
		return content;
	}

}
