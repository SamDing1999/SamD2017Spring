		return this.col;
	}

	@Override
	public Cell getCell(Location loc)
	{
		// TODO Auto-generated method stub
		return null;
	}

	
	public String getGridText()
	{
		
		for(int i = 0 ; i < 20; i++){
			for(int j = 0; j < 12; j++){
				
			}
			System.out.println();
		}
		
		return null;
	}

}
